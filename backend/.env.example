# Порт сервера
PORT=3000

# Токен для доступа к админ-панели (сгенерируйте случайный токен)
ADMIN_TOKEN=your-secure-token-here

# SMTP настройки для отправки email уведомлений
# Оставьте пустыми, если не нужны email уведомления
SMTP_HOST=smtp.yandex.ru
SMTP_PORT=465
SMTP_SECURE=true
SMTP_USER=your-email@yandex.ru
SMTP_PASS=your-app-password
SMTP_FROM="АС-ФИД" <noreply@as-feed.ru>

# Email для получения уведомлений о новых заявках
NOTIFICATION_EMAIL=info@as-feed.ru
